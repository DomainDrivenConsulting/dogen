<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">

    <!--[
    .NAME: odb.targets

    .SYNOPSIS:
        Generates all of the ODB files for this project.

    .DESCRIPTION:
        Calls the ODB compiler against each of the files in this Dogen model that require
        ODB compilation. Also moves the generated files to their correct location.

    .INPUTS:

    .OUTPUTS:

  ]-->

  <Target Name="odb_northwind"
          DependsOnTargets="odb_northwind_categories;
                            odb_northwind_class_0;
                            odb_northwind_customers;
                            odb_northwind_employees;
                            odb_northwind_order_details;
                            odb_northwind_order_details_key;
                            odb_northwind_orders;
                            odb_northwind_products;
                            odb_northwind_shippers;
                            odb_northwind_suppliers">
    <Message Importance="high" Text="ODB northwind "/>
  </Target>

  <Target Name="odb_northwind_categories">
    <Message Importance="high" Text="ODB categories"/>
    <Exec WorkingDirectory="$(OutputDirectory)" Command="&quot;$(OdbPath)&quot; -D_SECURE_SCL=0 $(OdbIncludeDirectories) --options-file $(MSBuildThisFileDirectory)odb/common_options.odb --options-file $(MSBuildThisFileDirectory)odb/categories_options.odb --output-dir $(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/odb $(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/types/categories.hpp" />
<Move
        SourceFiles="$(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/odb/categories-odb-pgsql.cxx"
        DestinationFolder="$(MSBuildThisFileDirectory)odb/" />
<Move
        SourceFiles="$(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/odb/categories-odb-oracle.cxx"
        DestinationFolder="$(MSBuildThisFileDirectory)odb/" />
  </Target>
  <Target Name="odb_northwind_class_0">
    <Message Importance="high" Text="ODB class_0"/>
    <Exec WorkingDirectory="$(OutputDirectory)" Command="&quot;$(OdbPath)&quot; -D_SECURE_SCL=0 $(OdbIncludeDirectories) --options-file $(MSBuildThisFileDirectory)odb/common_options.odb --options-file $(MSBuildThisFileDirectory)odb/package/class_0_options.odb --output-dir $(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/odb/package $(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/types/package/class_0.hpp" />
<Move
        SourceFiles="$(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/odb/package/class_0-odb-pgsql.cxx"
        DestinationFolder="$(MSBuildThisFileDirectory)odb/package/" />
<Move
        SourceFiles="$(MSBuildThisFileDirectory)../include/dogen/test_models/northwind/odb/package/class_0-odb-oracle.cxx"
        DestinationFolder="$(MSBuildThisFileDirectory)odb/package/" />
  </Target>
