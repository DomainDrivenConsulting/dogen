{
  "documentation": "Houses classes common to all transformations.\n",
  "tagged_values": {
    "masd.injection.dia.comment": "true",
    "masd.injection.model_modules": "masd.dogen.tracing",
    "masd.injection.input_language": "cpp",
    "masd.injection.reference": "cpp.builtins",
    "masd.injection.reference": "cpp.boost",
    "masd.injection.reference": "cpp.std",
    "masd.injection.reference": "masd.dogen",
    "masd.injection.reference": "masd.dogen.annotations",
    "masd.extraction.ignore_files_matching_regex": ".*/CMakeLists.txt",
    "masd.extraction.ignore_files_matching_regex": ".*/test/.*",
    "masd.extraction.ignore_files_matching_regex": ".*/tests/.*",
    "masd.extraction.delete_extra_files": "true",
    "masd.extraction.force_write": "false",
    "masd.extraction.cpp.enabled": "true",
    "masd.extraction.cpp.standard": "c++-17",
    "masd.extraction.cpp.msbuild.enabled": "false",
    "masd.extraction.cpp.visual_studio.project.enabled": "false",
    "masd.extraction.cpp.visual_studio.solution.enabled": "false",
    "masd.extraction.csharp.enabled": "false",
    "masd.annotations.profile": "dogen.disable_common_facets"
  },
  "elements": [
    {
      "name": "tracer",
      "stereotypes": [
        "masd::handcrafted"
      ],
      "tagged_values": {
        "masd.extraction.cpp.types.forward_declarations.enabled": "true"
      },
      "fallback_element_type": "masd::object"
    },
    {
      "name": "metrics",
      "fallback_element_type": "masd::object",
      "attributes": [
        {
          "name": "transform_id",
          "type": "std::string"
        },
        {
          "name": "model_id",
          "type": "std::string"
        },
        {
          "name": "guid",
          "type": "std::string"
        },
        {
          "name": "start",
          "type": "unsigned long"
        },
        {
          "name": "end",
          "type": "unsigned long"
        },
        {
          "name": "children",
          "type": "std::list<boost::shared_ptr<metrics>>"
        }
      ]
    },
    {
      "name": "metrics_builder",
      "stereotypes": [
        "masd::handcrafted"
      ],
      "fallback_element_type": "masd::object"
    },
    {
      "name": "metrics_printer",
      "stereotypes": [
        "masd::handcrafted"
      ],
      "fallback_element_type": "masd::object"
    },
    {
      "name": "tracing_error",
      "documentation": "An error occurred when trying to generate tracing data.\n",
      "stereotypes": [
        "masd::exception"
      ],
      "fallback_element_type": "masd::object"
    },
    {
      "name": "scoped_tracer",
      "stereotypes": [
        "masd::handcrafted"
      ],
      "fallback_element_type": "masd::object"
    }
  ]
