<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">

    <!--[
        .NAME: odb.targets

        .SYNOPSIS:
            Generates all of the ODB files for this project.

        .DESCRIPTION:
            Calls the ODB compiler against each of the files in this Dogen model that require
            ODB compilation. Also moves the generated files to their correct location.

        .INPUTS:

        .OUTPUTS:
    ]-->
    <Target Name="odb_dogen_relational"
          DependsOnTargets="odb_dogen_relational_attribute;
                            odb_dogen_relational_attribute_id;
                            odb_dogen_relational_element;
                            odb_dogen_relational_element_id;
                            odb_dogen_relational_json;
                            odb_dogen_relational_log;
                            odb_dogen_relational_model;
                            odb_dogen_relational_model_id;
                            odb_dogen_relational_model_set;
                            odb_dogen_relational_model_set_id;
                            odb_dogen_relational_run;
                            odb_dogen_relational_run_id;
                            odb_dogen_relational_transform;
                            odb_dogen_relational_transform_components;
                            odb_dogen_relational_transform_components_key;
                            odb_dogen_relational_transform_id;
                            odb_dogen_relational_transform_model;
                            odb_dogen_relational_transform_model_set;
                            odb_dogen_relational_xml">
        <Message Importance="high" Text="ODB dogen_relational "/>
    </Target>

    <Target Name="odb_dogen_relational_attribute">
        <Message Importance="high" Text="ODB attribute"/>
        <Exec WorkingDirectory="$(OutputDirectory)" Command="&quot;$(OdbPath)&quot; -D_SECURE_SCL=0 $(OdbIncludeDirectories) --options-file $(MSBuildThisFileDirectory)odb/common_options.odb --options-file $(MSBuildThisFileDirectory)odb/tracing/attribute_options.odb --output-dir $(MSBuildThisFileDirectory)../include/dogen.relational/odb/tracing $(MSBuildThisFileDirectory)../include/dogen.relational/types/tracing/attribute.hpp" />
        <Move
            SourceFiles="$(MSBuildThisFileDirectory)../include/dogen.relational/odb/tracing/attribute-odb.cxx"
            DestinationFolder="$(MSBuildThisFileDirectory)odb/tracing/" />
    </Target>
