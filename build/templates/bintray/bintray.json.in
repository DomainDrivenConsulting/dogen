{
    "package": {
        "name": "Dogen",
        "repo": "Dogen",
        "subject": "domaindrivenconsulting",
        "desc": "Generates C++-11 source code for Model Driven Software Development.",
        "website_url": "https://github.com/DomainDrivenConsulting/dogen",
        "issue_tracker_url": "https://github.com/DomainDrivenConsulting/dogen/issues",
        "vcs_url": "https://github.com/DomainDrivenConsulting/dogen",
        "github_use_tag_release_notes": true,
        "licenses": ["GPL-3.0"],
        "labels": ["code generation"],
        "public_download_numbers": true,
        "public_stats": true
    },
    "version": {
        "name": "${DOGEN_VERSION}",
        "desc": "Sprint ${DOGEN_MINOR_VERSION}",
        "released": "${BUILD_DATE}",
        "vcs_tag": "v${DOGEN_VERSION}",
        "gpgSign": false
    },

    "files":
        [
            { "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-applications.deb", "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-applications.deb" },
            { "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-headers.deb", "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-headers.deb" },
            { "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-libraries.deb", "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-libraries.deb" },
            { "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-tests.deb", "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-tests.deb" },
            { "includePattern": "${DOGEN_PACKAGE_DIR}/dogen-${DOGEN_VERSION}-Darwin-x86_64.dmg", "uploadPattern": "/${DOGEN_VERSION}/dogen-${DOGEN_VERSION}-Darwin-x86_64.dmg" }
        ],
    "publish": true
}
